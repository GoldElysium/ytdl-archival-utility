{
	"name": "ytdl-archival-utility",
	"description": "An easy to use YouTube downloader, mainly for VTuber content",
	"version": "0.0.3",
	"author": "GoldElysium",
	"bin": {
		"vdl": "./bin/run"
	},
	"bugs": {
		"url": "https://github.com/GoldElysium/ytdl-archival-utility/issues"
	},
	"devDependencies": {
		"@oclif/test": "3.0.2",
		"@tsconfig/node-lts": "^18.12.5",
		"@types/chai": "4.3.13",
		"@types/cli-progress": "3.11.3",
		"@types/command-exists": "1.2.1",
		"@types/debug": "4.1.9",
		"@types/ffmpeg-static": "3.0.1",
		"@types/ffprobe-static": "2.0.1",
		"@types/fluent-ffmpeg": "2.1.22",
		"@types/inquirer": "8.2.5",
		"@types/langs": "2.0.2",
		"@types/mocha": "10.0.2",
		"@types/node": "20.8.6",
		"@typescript-eslint/eslint-plugin": "6.7.5",
		"@typescript-eslint/parser": "6.7.5",
		"chai": "4.4.1",
		"eslint": "8.51.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.1.0",
		"eslint-config-oclif": "5.0.0",
		"eslint-config-oclif-typescript": "3.0.5",
		"eslint-plugin-import": "2.28.1",
		"makensis": "^2.0.8",
		"mocha": "10.2.0",
		"np": "8.0.4",
		"nyc": "15.1.0",
		"oclif": "^4.0.2",
		"shx": "^0.3.4",
		"ts-node": "10.9.1",
		"typescript": "5.2.2"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/GoldElysium/ytdl-archival-utility",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "bin/run",
	"oclif": {
		"bin": "vdownloader",
		"additionalHelpFlags": [
			"-h"
		],
		"additionalVersionFlags": [
			"-v"
		],
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-version"
		],
		"default": ".",
		"commands": "./dist/commands"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GoldElysium/ytdl-archival-utility.git"
	},
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts --config .eslintrc.js",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "pnpm lint",
		"prepack": "pnpm build && oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"types": "dist/index.d.ts",
	"dependencies": {
		"@oclif/core": "^3.2.1",
		"@oclif/plugin-help": "^6.0.2",
		"@oclif/plugin-version": "^2.0.3",
		"axios": "^1.5.1",
		"chalk": "^4.1.2",
		"cli-progress": "^3.12.0",
		"command-exists": "^1.2.9",
		"debug": "^4.3.4",
		"ffmpeg-static": "^5.2.0",
		"ffprobe-static": "^3.1.0",
		"fluent-ffmpeg": "^2.1.2",
		"inquirer": "^8.2.5",
		"langs": "^2.0.0",
		"sanitize-filename": "^1.6.3",
		"tslib": "^2.6.2",
		"ytdl-core": "^4.11.5"
	}
}
