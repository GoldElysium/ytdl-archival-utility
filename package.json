{
	"name": "ytdl-archival-utility",
	"description": "An easy to use YouTube downloader, mainly for VTuber content",
	"version": "0.0.2",
	"author": "GoldElysium @GoldElysium",
	"bin": {
		"vdl": "dist/run"
	},
	"bugs": {
		"url": "https://github.com/GoldElysium/ytdl-archival-utility/issues"
	},
	"devDependencies": {
		"@oclif/test": "1.2.8",
		"@types/chai": "4.2.22",
		"@types/cli-progress": "3.9.2",
		"@types/command-exists": "1.2.0",
		"@types/debug": "4.1.7",
		"@types/ffmpeg-static": "3.0.0",
		"@types/ffprobe-static": "2.0.0",
		"@types/fluent-ffmpeg": "2.1.18",
		"@types/inquirer": "8.1.3",
		"@types/langs": "2.0.1",
		"@types/mocha": "9.1.1",
		"@types/node": "16.10.2",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"chai": "4.3.4",
		"eslint": "7.32.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-config-airbnb-typescript": "14.0.1",
		"eslint-config-oclif": "4.0.0",
		"eslint-config-oclif-typescript": "1.0.2",
		"eslint-plugin-import": "2.24.2",
		"makensis": "^1.1.3",
		"mocha": "9.2.2",
		"np": "7.5.0",
		"nyc": "15.1.0",
		"oclif": "^3.2.19",
		"shx": "^0.3.4",
		"ts-node": "10.2.1",
		"typescript": "4.4.3"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/GoldElysium/ytdl-archival-utility",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"oclif": {
		"bin": "vdl",
		"additionalHelpFlags": [
			"-h"
		],
		"additionalVersionFlags": [
			"-v"
		],
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-version"
		],
		"default": ".",
		"commands": "./dist/commands"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GoldElysium/ytdl-archival-utility.git"
	},
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts --config .eslintrc.js",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "pnpm lint",
		"prepack": "pnpm build && oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"types": "dist/index.d.ts",
	"dependencies": {
		"@oclif/core": "^1.19.1",
		"@oclif/plugin-help": "^3.3.1",
		"@oclif/plugin-version": "^1.1.3",
		"chalk": "^4.1.2",
		"cli-progress": "^3.11.2",
		"command-exists": "^1.2.9",
		"debug": "^4.3.4",
		"ffmpeg-static": "^4.4.1",
		"ffprobe-static": "^3.1.0",
		"fluent-ffmpeg": "^2.1.2",
		"got": "^11.8.5",
		"inquirer": "^8.2.4",
		"langs": "^2.0.0",
		"sanitize-filename": "^1.6.3",
		"tslib": "^2.4.0",
		"ytdl-core": "^4.11.2"
	}
}
